&i2c0 {
    status = "okay";
    clock-frequency = <I2C_BITRATE_STANDARD>;
	SI7006@40 {
		compatible = "silabs,si7006";
		reg = <0x40>;
		label = "SI7006";
	};
};

&i2c0 {
    status = "okay";
    clock-frequency = <I2C_BITRATE_STANDARD>;
	MS5837@76 {
		compatible = "meas,ms5837";
		reg = <0x76>;
		label = "MS5837";
	};
};
